programa {
	funcao inicio() {
		real A,B,C,D,R,S
		
		escreva ("escreva o valor de A : ")
		    leia (A)
		escreva ("escreva o valor de B : ")
		    leia (B)
		escreva ("escreva o valor de C : ")
		    leia (C)
		    
		   R = (A + B)*(A+B)
		   S = (B+C)*(B+C)
		   D = (R + S)/2
		   
		   escreva (" O valor de D é: ", D)
	}
}
